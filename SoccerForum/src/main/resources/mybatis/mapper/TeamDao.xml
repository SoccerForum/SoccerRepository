<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.SoccerForum.dao.TeamDao">
<select id="getMemberById" parameterType="int" resultType="Member">
	select *
		from members
		where teamId = #{id}
</select>
<resultMap type="Team" id="getTeamById">
	<id column="id" property="id"/>
	<collection property="member" ofType="Member" column="id" select="getMemberById"/>
</resultMap>
<select id="findById" parameterType="int" resultMap="getTeamById">
	select * 
	from teams
		where id = #{id}
</select>
</mapper>